# Example .env file for Cotabot
# Copy this to .env and fill in your values

# Discord Bot Configuration
DISCORD_TOKEN=your_discord_bot_token_here

# BattleMetrics API
BATTLEMETRICS_TOKEN=your_battlemetrics_api_token_here

# Google Sheets API (optional)
GOOGLE_SHEET_KEY=your_google_sheet_key_here

# Web Admin Panel Configuration
WEB_ADMIN_API_KEY=cotabot-admin-2024
WEB_PANEL_PORT=5000

# Database Configuration
DATABASE_PATH=cotabot_dev.db

# Environment
FLASK_ENV=production
PYTHONUNBUFFERED=1
